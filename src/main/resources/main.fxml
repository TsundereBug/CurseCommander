<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.web.WebView?>

<BorderPane prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.tsunderebug.cursecommander.view.MainController">
   <center>
      <VBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="400.0" prefWidth="600.0" BorderPane.alignment="CENTER">
         <children>
            <HBox maxHeight="15.0" prefHeight="15.0" prefWidth="600.0">
               <children>
                  <TextField fx:id="searchField" maxWidth="1.7976931348623157E308" prefWidth="450.0" />
                  <ChoiceBox fx:id="versionSelector" prefWidth="150.0" />
               </children>
            </HBox>
            <ListView fx:id="mainList" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="375.0" prefWidth="200.0" />
         </children>
      </VBox>
   </center>
   <bottom>
      <WebView fx:id="webView" prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER" />
   </bottom>
   <right>
      <VBox prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
         <children>
            <Button fx:id="downloadButton" mnemonicParsing="false" prefWidth="200.0" text="Download" />
            <ListView fx:id="selectedList" prefHeight="375.0" prefWidth="200.0" />
         </children>
      </VBox>
   </right>
</BorderPane>
